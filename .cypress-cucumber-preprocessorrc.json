{
  "stepDefinitions": ["cypress/e2e/step_definitions/**/*.js"],

  "filterSpecs": true,
  "omitFiltered": true,

  "messages": {
    "enabled": true,
    "output": "reports/cucumber-messages.ndjson"
  },
  "json": {
    "enabled": true,
    "output": "reports/cucumber-report.json"
  },
  "html": {
    "enabled": true,
    "output": "reports/cucumber-report.html"
  }
}
